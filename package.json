{
  "name": "smash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.prod react-native run-android --verbose --variant=prodDebug",
    "android-dev": "ENVFILE=.env.dev react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android-qa": "ENVFILE=.env.qa react-native run-android --variant=qaDebug --appIdSuffix=qa",
    "win-qa": "SET ENVFILE=.env.qa && npx react-native run-android --variant=qaDebug --appIdSuffix=qa",
    "win-dev": "SET ENVFILE=.env.dev && npx react-native run-android --variant=devDebug --appIdSuffix=dev",
    "ios": "ENVFILE=.env.prod npx react-native run-ios --scheme Smash simulator\"iphone11\" --configuration Release",
    "ios-dev": "ENVFILE=.env.dev npx react-native run-ios --scheme Dev simulator\"iphone11\" --configuration Dev.Debug",
    "ios-qa": "ENVFILE=.env.qa npx react-native run-ios --scheme Qa simulator\"iphone11\" --configuration Qa.Release",
    "bundle-ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "start": "react-native start",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "clean": "react-native-clean-project"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
